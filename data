  <script type="text/javascript">
   const months = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];
  const DefaultImageUrlForError = '';

  function displayVideoTabs(that){
    let auctionYear = $(that).attr('data-year');
    $('#videosData').slick('unslick');
    $.ajax({
            url: "/auction-videos-by-years/"+auctionYear+"/1",
            type: "GET",
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            },
            dataType: 'JSON',
            success: function (response) {
                if (response.status == true) {

                    myVideos = response.data;
                    myVideos = $.parseJSON(myVideos);
                    if(myVideos.status != false){
                      $('.videosvideoTab'+auctionYear).html(' ');
                    }
                    console.log(myVideos.data);
                    $.each(myVideos.data, function( index, value ) {
                      var duration = value.duration;
                      var minutes = (Math.floor(duration / 60) < 10)?"0"+Math.floor(duration / 60):Math.floor(duration / 60);
                      var seconds = (duration % 60 < 10 )?"0"+duration % 60:duration % 60;
                      const date = new Date(value.date);
                      const newDate = (date.getDate())+" "+months[date.getMonth()]+", "+date.getFullYear();
                      if (value.views === undefined)
                        value.views = value.view_count;
                      if (value.views === undefined)
                        value.views = value.views_count;
                      if (value.views === undefined)
                        value.views = 0;

                    $('.videosvideoTab'+auctionYear).append('<div class="ap-common-news" style="" aria-hidden="true" tabindex="0"><a href="javascript:;"  data-mediaId="'+value.mediaId+'" data-videoId="'+value.id+'" data-videoTitle="'+value.title+'" data-description="'+value.description+'" data-videoView="'+value.views+'" data-videoDate="'+date.getDate()+' '+months[date.getMonth()]+','+date.getFullYear()+'" data-videoslug="'+value.id+'/'+value.titleUrlSegment+'"  class="playerpopup" tabindex="-1"><div class="ap-slide-content col-100 floatLft relative"><div class="ap-img-box col-100 floatLft relative"><img src="'+value.imageUrl+'" alt=""><span class="ap-play-video-btn absolute " tabindex="-1"><img src="https://www.iplt20.com/assets/images/Play.png" alt=""></span></div><div class="ap-img-content col-100 floatLft"><div class="ap-date-wrp flexDisplay justifySpace alignCenter"><span>'+date.getDate()+" "+months[date.getMonth()]+","+date.getFullYear()+'</span><span><i class="fa fa-eye" aria-hidden="true"></i>'+value.views+'</span></div><h3 class="ap-slide-video-title col-100 floatLft textCenter textTwoLine">'+value.title+'</h3></div><div class="ap-time-video absolute" data-duration="617">'+minutes+':'+seconds+'</div></div></a></div>');
                  });

                    $('#videosData'+auctionYear).slick({
                      slidesToShow: 1,
                      slidesToScroll: 1,
                      arrows: true,
                      dots: false,
                      speed: 1000,
                  });

                    if(myVideos.data.length >= 20){
                      $('.videosvideoTab'+auctionYear).after( '<div class="col-lg-12 col-md-12 col-xs-12"><div class="vn-ticnbtn text-center pb-4"><a onclick="loadMoreFunctionality(this)" id="loadMoreBtn'+auctionYear+'" data-page="2" data-year="'+auctionYear+'" class="load-more-pagination loadmoredatabtn">Load More</a></div></div>');
                    }
                } else if (response.status == false) {
                    $('.videosvideoTab'+auctionYear).html("No data Found");
                }
            },
            error: function (response) {
            }
        });
  }

  function loadMoreFunctionality(that) {
    let auctionYear = $(that).attr('data-year');
    let pageNumber = $(that).attr('data-page');
    $.ajax({
            url: "/auction-videos-by-years/"+auctionYear+"/"+pageNumber,
            type: "GET",
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            },
            dataType: 'JSON',
            success: function (response) {
                if (response.status == true) {
                    $(that).attr('data-page', parseInt(pageNumber)+1)
                    myVideos = response.data;
                    myVideos = $.parseJSON(myVideos);
                    console.log(myVideos);
                    if(myVideos.status==false || myVideos.status=='false'){
                      $('#loadMoreBtn'+auctionYear).hide();
                    }
                    $.each(myVideos.data, function( index, value ) {
                      var duration = value.duration;
                      var minutes = (Math.floor(duration / 60) < 10)?"0"+Math.floor(duration / 60):Math.floor(duration / 60);
                      var seconds = (duration % 60 < 10 )?"0"+duration % 60:duration % 60;
                      const date = new Date(value.date);
                      const newDate = (date.getDate())+" "+months[date.getMonth()]+", "+date.getFullYear();
                      if (value.views === undefined)
                        value.views = value.view_count;
                      if (value.views === undefined)
                        value.views = value.views_count;
                      if (value.views === undefined)
                        value.views = 0;

                    $('.videosvideoTab'+auctionYear).append('<div class="ap-common-news" style="" aria-hidden="true" tabindex="0"><a href="javascript:;"  data-mediaId="'+value.mediaId+'" data-videoId="'+value.id+'" data-videoTitle="'+value.title+'" data-description="'+value.description+'" data-videoView="'+value.views+'" data-videoDate="'+date.getDate()+' '+months[date.getMonth()]+','+date.getFullYear()+'" data-videoslug="'+value.id+'/'+value.titleUrlSegment+'"  class="playerpopup" tabindex="-1"><div class="ap-slide-content col-100 floatLft relative"><div class="ap-img-box col-100 floatLft relative"><img src="'+value.imageUrl+'" alt=""><span class="ap-play-video-btn absolute " tabindex="-1"><img src="https://www.iplt20.com/assets/images/Play.png" alt=""></span></div><div class="ap-img-content col-100 floatLft"><div class="ap-date-wrp flexDisplay justifySpace alignCenter"><span>'+date.getDate()+" "+months[date.getMonth()]+","+date.getFullYear()+'</span><span><i class="fa fa-eye" aria-hidden="true"></i>'+value.views+'</span></div><h3 class="ap-slide-video-title col-100 floatLft textCenter textTwoLine">'+value.title+'</h3></div><div class="ap-time-video absolute" data-duration="617">'+minutes+':'+seconds+'</div></div></a></div>');
                  });
                  if(myVideos.data.length < 19){
                    $('#loadMoreBtn'+auctionYear).hide();
                  }

                } else if (response.status == false) {
                    $('#loadMoreBtn'+auctionYear).hide();
                }
            },
            error: function (response) {
            }
        });
  }

  function loadMoreFunctionalityimage(that) {
    var extraForTwo = ''
    let auctionYear = $(that).attr('data-year');
    let pageNumber = $(that).attr('data-page');
    $('.ap-photo-slider').slick('unslick');
    $('.ap-photo-cnt-slider').slick('unslick');
    $.ajax({
            url: "/auction-images-by-years/"+auctionYear+"/"+pageNumber,
            type: "GET",
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            },
            dataType: 'JSON',
            success: function (response) {
                // console.log(response);
                if (response.status == true) {
                    $(that).attr('data-page', parseInt(pageNumber)+1)
                    myImages = response.data;
                    myImages = $.parseJSON(myImages);
                    $startpoint = pageNumber*20;
                    $startpoint2 = $startpoint+pageNumber*20;
                    $.each(myImages.data, function( index, value ) {
                      // console.log(value);
                      const date = new Date(value.date);
                      const newDate = (date.getDate())+" "+months[date.getMonth()]+", "+date.getFullYear();
                    $('#addImageToauction22').append('<div class="ap-photo-inner-wrp" data-id="'+$startpoint+'"> <a href="#ap-photo-pop" class="ap-phto-click" data-id="'+$startpoint+'"><img src="'+value.imageUrl+'" alt=""></a></div>');
                    $('#imageListingPartTwo').append('<div class="ap-phto-item slick-slide" data-slick-index="'+$startpoint+'" aria-hidden="true" tabindex="-1" style="width: 736px;"><img src="'+value.imageUrl+'" alt=""></div>');

                    $('#imageListingPartThree').append('<div class="ap-pht-cnt-item"><p class="col-100 floatLft">'+value.description+'</p><span class="col-100 floatLft">'+newDate+'</span><div class="share-photo"><a href="//www.facebook.com/sharer.php?u=https://www.iplt20.com/auction" target="_blank" class="so facebook dsna"><i class="fa fa-facebook-official"></i></a><a href="//twitter.com/share?url=https://www.iplt20.com/auction" target="_blank" class="so tweeter dsna"><i class="fa fa fa-twitter"></i></a><a href="#/" data-urlcopy="https://www.iplt20.com/auction" class="so link dsna copyUrl" ><i class="fa fa-link" aria-hidden="true"></i></a><a href="#/" class="so share dsn"><i class="fa fa-share-alt text-primary" aria-hidden="true"></i></a></div></div>');
                    $startpoint++;
                  });
                  $('.ap-photo-slider').slick({
                      slidesToShow: 1,
                      slidesToScroll: 1,
                      arrows: true,
                      dots: false,
                      speed: 1000,
                      asNavFor: '.ap-photo-cnt-slider',
                  });

                  $('.ap-photo-cnt-slider').slick({
                      slidesToShow: 1,
                      slidesToScroll: 1,
                      arrows: false,
                      dots: false,
                      speed: 1000,
                      asNavFor: '.ap-photo-slider',
                  });

                  $('.ap-phto-click').magnificPopup({

                    type: 'inline',
                    preloader: false,
                    focus: '#name',
                    closeOnBgClick: false,
                    callbacks: {
                      beforeOpen: function () {

                        $('.ap-photo-inner-wrp').on('click', function() {
                          // alert(3);
                            var mp = $.magnificPopup.instance;
                            // console.log(mp.index);
                            var current = mp.index;
                            $('.ap-photo-slider').slick('unslick');
                            $('.ap-photo-cnt-slider').slick('unslick');

                            $('.ap-photo-slider').slick({
                                slidesToShow: 1,
                                slidesToScroll: 1,
                                arrows: true,
                                dots: false,
                                speed: 1000,
                                asNavFor: '.ap-photo-cnt-slider',
                            });

                            $('.ap-photo-cnt-slider').slick({
                                slidesToShow: 1,
                                slidesToScroll: 1,
                                arrows: false,
                                dots: false,
                                speed: 1000,
                                asNavFor: '.ap-photo-slider',
                            });

                            $('.ap-photo-slider').slick('slickGoTo', parseInt(current));
                            $('.ap-photo-cnt-slider').slick('slickGoTo', parseInt(current));
                        });

                        /*var mp = $.magnificPopup.instance;
                        console.log(mp.index);
                        var current = mp.index;
                        $('.ap-photo-slider').slick('unslick');
                        $('.ap-photo-cnt-slider').slick('unslick');
                        setTimeout(function () {
                          $('.ap-photo-slider').slick({
                              slidesToShow: 1,
                              slidesToScroll: 1,
                              arrows: true,
                              dots: false,
                              speed: 1000,
                              asNavFor: '.ap-photo-cnt-slider',
                          });
                        }, 100);
                        setTimeout(function () {
                          $('.ap-photo-cnt-slider').slick({
                              slidesToShow: 1,
                              slidesToScroll: 1,
                              arrows: false,
                              dots: false,
                              speed: 1000,
                              asNavFor: '.ap-photo-slider',
                          });
                        }, 50);
                        $('.ap-photo-slider').slick('slickGoTo', parseInt(current));
                        $('.ap-photo-cnt-slider').slick('slickGoTo', parseInt(current));*/
                      }
                    }
                  });

                  $('.copyUrl').on('click', function(){
                      var shareurl = $(this).attr('data-urlcopy');
                      var inputc = document.body.appendChild(document.createElement("input"));
                      inputc.value = shareurl;
                      inputc.focus();
                      inputc.select();
                      document.execCommand('copy');
                      inputc.parentNode.removeChild(inputc);
                      document.getElementById("cl").innerHTML = "Copied !";
                      setTimeout(function() {
                          document.getElementById("cl").innerHTML = "Copy <b>Link</b>";
                      }, 1000);
                  });

                  if(myImages.data.length < 19){
                    $('#loadMoreBtnimage2022').hide();
                  }

                } else if (response.status == false) {
                    $('#loadMoreBtnimage2022').hide();
                }
            },
            error: function (response) {
            }
        }).done(function() {


        });
  }

  function autoRefreshPage()
  {
      window.location = window.location.href;
  }
