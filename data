  <script>
   $('.copyUrl1').on('click', function(){
    $(".ctc .c").text("Copied!");
	var shareurl = $(this).attr('data-shareurl');
    var dummy = $('<input>').val(shareurl).appendTo('body').select();
    dummy.focus();
    document.execCommand('copy');
	document.getElementById("cl").innerHTML = "Copied !";
	// setTimeout(function() {
	// 	document.getElementById("cl").innerHTML = "Copy <b>Link</b>";
	// }, 1000);
});
  </script>
  <script type="text/javascript">
   $(document).ready(function() {
            $('li:has(ul li.activate)').addClass('active');

            $(".change-tick").click(function() {
                $(this).attr("src", app_url + '/assets/images/tick.svg');
                $(this).addClass("nonClrImage");
            });
            $('.ap-searchClick').click(function() {
                var imgSrcForMenu = $('.ap-searchClick img').attr('src');
                if (imgSrcForMenu == "https://www.iplt20.com/assets/images/magnify.png") {
                    $('.ap-searchClick img').attr('src', "https://www.iplt20.com/assets/images/close-add.png")
                } else {
                    $('.ap-searchClick img').attr('src', "https://www.iplt20.com/assets/images/magnify.png")
                }
                $('#searchTabForHeaderMenu').toggleClass('d-none');
            });
        });

        // Script for loader
        // $(window).on('load', function() {
        //     $('#hloader').hide();
        // })
  </script>
